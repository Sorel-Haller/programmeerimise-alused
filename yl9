n1 = int(input("Anna esimene küljepikkus:"))
n2 = int(input("Anna teine küljepikkus:"))
n3 = int(input("Anna kolmas küljepikkus:"))

if n1 == n2 and n1 == n3:
    print("See on võrdkülgne kolmnurk")

if n1 == n2 and n1 != n3:
    print("See on võrdhaarne kolmnurk")

if n2 == n3 and n1 != n3:
    print("See on võrdhaarne kolmnurk")

if n1 != n2 != n3:
    print("See on erikülgne kolmnurk")


numbrid = [n1, n2, n3]
max1 = numbrid[0]
for ele in numbrid:
    if ele>max1:
        max1=ele

min1 = numbrid[0]
for ele in numbrid:
    if ele<min1:
        min1=ele

min2 = numbrid[1]
for ele in numbrid:
    if ele<min2:
        min2=ele

if (min1 + min2) <= max1:
    print ("See kolmnurk ei eksisteeri")
else: print("See kolmnurk eksisteerib")